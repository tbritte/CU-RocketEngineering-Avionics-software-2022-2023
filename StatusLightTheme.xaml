<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">



    <Style TargetType="{x:Type Ellipse}"
           x:Key="BaseLightTheme">
        <Style.Setters>

            <Setter Property="Height" Value="55"/>
            <Setter Property="Width" Value="55"/>
            <Setter Property="Margin" Value="15,10"/>
            <Setter Property="VerticalAlignment" Value="Top"/>
            <Setter Property="Stroke" Value="Black"/>
            <Setter Property="StrokeThickness" Value="2"/>

        </Style.Setters>

    </Style>

    <Style TargetType="{x:Type Ellipse}"
           x:Key="StatusLightTheme0"
           BasedOn="{StaticResource BaseLightTheme}">
        
        <Style.Triggers>
            <DataTrigger Binding="{Binding Path=IndicatorLights[0]}" Value="True">
                <Setter Property="Fill" Value="#08FF08"/>
            </DataTrigger>
            <DataTrigger Binding="{Binding Path=IndicatorLights[0]}" Value="False">
                <Setter Property="Fill" Value="#FF0000"/>
            </DataTrigger>
        </Style.Triggers>
    </Style>

